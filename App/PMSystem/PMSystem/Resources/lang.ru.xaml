<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <!--Sign Up and Log In-->
    <sys:String x:Key="LoginTitle">Вход</sys:String>
    <sys:String x:Key="LogInText">Войти</sys:String>
    <sys:String x:Key="SignUpTitle">Создать аккаунт</sys:String>
    <sys:String x:Key="UsernameLabel">Имя пользователя</sys:String>
    <sys:String x:Key="EmailLabel">Почта</sys:String>
    <sys:String x:Key="PasswordLabel">Пароль</sys:String>
    <sys:String x:Key="DontHaveAccountText">Нет аккаунта?</sys:String>
    <sys:String x:Key="AlreadyHaveAccount">Уже есть аккаунт?</sys:String>
    <sys:String x:Key="SignUpText">Зарегистрироваться</sys:String>
    <!--Main Page-->
    <sys:String x:Key="MainPageTitle">Главная страница</sys:String>
    <sys:String x:Key="ProjectButton">Проекты</sys:String>
    <sys:String x:Key="TaskButton">Задачи</sys:String>
    <sys:String x:Key="StatisticsButton">Статистика</sys:String>
    <sys:String x:Key="UserButton">Пользователи</sys:String>
    <sys:String x:Key="WelcomeText">Добро пожаловать!</sys:String>
    <sys:String x:Key="SideText">Здесь будет отображаться содержимое выбранного раздела.</sys:String>
    <sys:String x:Key="ExitButton">Выйти</sys:String>
    <sys:String x:Key="ProjectMoreButton">Подробнее</sys:String>
    <sys:String x:Key="TasksDetailsButton">Детали</sys:String>
    <sys:String x:Key="UserInfoButton">Информация</sys:String>
    <sys:String x:Key="SearchButtonTitle">Поиск</sys:String>
    <!--User Page-->
    <sys:String x:Key="UserPageTitle">Профиль пользователя</sys:String>
    <sys:String x:Key="UserName">Имя</sys:String>
    <sys:String x:Key="UserEmail">Почта</sys:String>
    <sys:String x:Key="UserPassword">Пароль</sys:String>
    <sys:String x:Key="UserPAgeCloseButton">Закрыть</sys:String>
</ResourceDictionary>
